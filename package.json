{
  "name": "react-relay-mutation",
  "version": "0.2.2",
  "description": "Higher-level React mutation API for Relay",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/relay-tools/react-relay-mutation.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "babel": "babel src --ignore **/__tests__ --delete-dir-on-start -x .js,.ts,.tsx",
    "build": "yarn babel -d lib && yarn babel -d es --env-name esm && yarn build:types",
    "build:fixtures": "npm run update-schema && npm run relay-compiler",
    "build:types": "tsc --emitDeclarationOnly --outDir lib && tsc --outDir es",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "format": "yarn eslint --fix && yarn prettier --write",
    "lint": "yarn eslint && yarn prettier --list-different && yarn typecheck",
    "prepublishOnly": "yarn run build",
    "prettier": "prettier --ignore-path .eslintignore '**/*.{json,css,md}'",
    "relay-compiler": "relay-compiler --watchman false --src test --schema test/fixtures/schema.graphql --language typescript --extensions js ts tsx",
    "release": "rollout",
    "tdd": "jest --watch",
    "test": "yarn build:fixtures && yarn lint && jest",
    "testonly": "jest",
    "typecheck": "tsc --noEmit && tsc --noEmit -p test",
    "update-schema": "ts-node --skip-project test/fixtures/updateSchema.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "*.{json,css,md}": "prettier --write --ignore-path .eslintignore"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "jsdom"
  },
  "release": {
    "publishDir": "lib"
  },
  "dependencies": {
    "@restart/hooks": "^0.3.15",
    "@types/react": ">=16.8.0",
    "@types/react-relay": ">=4.0.0",
    "@types/relay-runtime": ">=4.0.0",
    "invariant": "^2.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-relay": ">=2.0.0",
    "relay-runtime": ">=2.0.0"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.3.4",
    "@4c/jest-preset": "^1.5.0",
    "@4c/rollout": "^2.1.6",
    "@4c/tsconfig": "^0.3.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/graphql-relay": "^0.6.0",
    "@types/invariant": "^2.2.32",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-jest": "^25.4.0",
    "babel-plugin-relay": "^9.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-4catalyzer-jest": "^2.0.6",
    "eslint-config-4catalyzer-react": "^1.0.6",
    "eslint-config-4catalyzer-typescript": "^1.1.8",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "graphql": "^15.0.0",
    "graphql-relay": "^0.6.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^9.0.0",
    "relay-compiler": "^9.0.0",
    "relay-compiler-language-typescript": "^12.0.1",
    "relay-local-schema": "^0.8.0",
    "relay-runtime": "^9.0.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
