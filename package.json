{
  "name": "react-relay-mutation",
  "version": "0.1.2",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/relay-tools/react-relay-mutation.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "babel": "babel src --ignore **/__tests__ --delete-dir-on-start -x .js,.ts,.tsx",
    "build:fixtures": "npm run update-schema && npm run relay-compiler",
    "build:types": "tsc --emitDeclarationOnly --outDir lib && tsc --outDir es",
    "build": "yarn babel -d lib && yarn babel -d es --env-name esm && yarn build:types",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "format": "yarn import-sort --write && yarn eslint && yarn prettier --write",
    "import-sort": "import-sort '**/src/**/*.{js,ts,tsx}' '!**/__generated__/*' '!**/*.d.ts'",
    "lint": "yarn import-sort --list-different && yarn eslint && yarn prettier --list-different && yarn typecheck",
    "prepublishOnly": "yarn run build",
    "prettier": "prettier --ignore-path .eslintignore '**/*.{json,css,md}'",
    "relay-compiler": "relay-compiler --watchman false --src test --schema test/fixtures/schema.graphql --language typescript --extensions js ts tsx",
    "release": "rollout",
    "tdd": "jest --watch",
    "test": "yarn build:fixtures && yarn lint && jest",
    "testonly": "jest",
    "typecheck": "tsc --noEmit && tsc --noEmit -p test",
    "update-schema": "ts-node --skip-project test/fixtures/updateSchema.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "importSort": {
    ".js, .ts, .tsx": {
      "parser": "@4c/import-sort/parser-ts",
      "style": "@4c/import-sort/style"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "import-sort --write",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write --ignore-path .eslintignore",
      "git add"
    ]
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "jsdom"
  },
  "release": {
    "publishDir": "lib"
  },
  "devDependencies": {
    "@4c/babel-preset": "^5.1.0",
    "@4c/import-sort": "^4.3.0",
    "@4c/jest-preset": "^1.3.0",
    "@4c/rollout": "^1.2.0",
    "@4c/tsconfig": "^0.1.1",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/enzyme": "^3.9.1",
    "@types/graphql": "^14.0.7",
    "@types/graphql-relay": "^0.4.9",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.6",
    "@types/react-dom": "^16.8.3",
    "@types/sinon": "^7.0.10",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-relay": "^3.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.12.0",
    "eslint-config-4catalyzer-jest": "^1.1.0",
    "eslint-config-4catalyzer-react": "^0.9.3",
    "eslint-config-4catalyzer-typescript": "^0.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "graphql": "^14.1.1",
    "graphql-relay": "^0.6.0",
    "husky": "^1.1.2",
    "import-sort-cli": "^6.0.0",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-relay": "^3.0.0",
    "relay-compiler": "^3.0.0",
    "relay-compiler-language-typescript": "^2.0.0",
    "relay-local-schema": "^0.8.0",
    "sinon": "^7.3.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": "^16.8.5",
    "react-relay": "^3.0.0"
  },
  "dependencies": {
    "@types/react": "^16.8.8",
    "@types/react-relay": "^1.3.12",
    "@types/relay-runtime": "^1.3.12",
    "invariant": "^2.2.4",
    "relay-runtime": "^3.0.0"
  }
}
